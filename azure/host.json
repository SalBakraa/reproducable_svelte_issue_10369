{
	"version": "2.0",

	"logging": {
		"logLevel": {
			"default": "Debug"
		},

		"fileLoggingMode": "always",

		"applicationInsights": {
			"samplingSettings": {
				"isEnabled": true,
				"excludedTypes": "Request"
			}
		}
	},

	"extensionBundle": {
		"id": "Microsoft.Azure.Functions.ExtensionBundle",
		"version": "[3.*, 4.0.0)"
	},

	"extensions": {
		"http": {
			"routePrefix": ""
		}
	}
}
